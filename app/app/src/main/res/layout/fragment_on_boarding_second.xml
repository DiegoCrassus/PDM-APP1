<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.fragments.onboarding.OnBoardingSecondFragment" >

        <ImageView
            android:id="@+id/imfrag2"
            android:layout_width="319dp"
            android:layout_height="321dp"
            android:layout_marginTop="70dp"
            android:layout_marginBottom="64dp"
            android:src="@drawable/logo_splashscreen"
            app:layout_constraintBottom_toTopOf="@+id/tvfrag2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvfrag2"
            style="?titleStyle"
            android:layout_width="377dp"
            android:layout_height="48dp"
            android:layout_marginBottom="280dp"
            android:text="@string/title_second_fragment"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>